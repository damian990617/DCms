{
    "name": "d-dcms/form-builder",
    "description": "The form package to manage the forms.",
    "keywords": ["laravel", "form"],
    "version": "0.1",
    "authors": [
        {
            "name": "Damian B",
            "email": "damianbialkowski8@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/database": "^5.6@dev|^6|^7",
        "illuminate/validation": "^5.6@dev|^6|^7",
        "laravelcollective/html": "^6.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Core\\src\\FormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Modules\\Core\\src\\FormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Core\\src\\FormBuilder\\": ""
        },
        "files": [
            "Helpers/helpers.php"
        ]
    }
}
