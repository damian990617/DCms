{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "cviebrock/eloquent-sluggable": "6.0.3",
        "facuz/laravel-themes": "^3.2",
        "fideloper/proxy": "^4.0",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "6.18.40",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.1",
        "maatwebsite/laravel-sidebar": "~2.1",
        "nwidart/laravel-modules": "^6.2",
        "silber/bouncer": "v1.0.0-rc.8",
        "spatie/laravel-medialibrary": "^7.0.0",
        "wikimedia/composer-merge-plugin": "dev-master",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^9.0",
        "fzaninotto/faker": "^1.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "Maatwebsite\\Sidebar\\SidebarSerivceProvider"
            ]
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ],
            "replace": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}